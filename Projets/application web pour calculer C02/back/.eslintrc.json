{
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
      "consistent-return": "off",
      "comma-dangle": ["error", "never"],
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "indent": ["error", 2],
      "linebreak-style": ["error", "unix"]
    }
  }
  